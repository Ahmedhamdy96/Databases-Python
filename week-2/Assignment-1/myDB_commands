CREATE TABLE Ages(
	name VARCHAR(128) , 
	age INTEGER 
	)
	
	DELETE FROM Ages ; 
	INSERT INTO Ages (name , age) VALUES ('Enis' , 28 ) ;
	INSERT INTO Ages (name , age) VALUES ('Caity' , 24 ) ;
	INSERT INTO Ages (name , age) VALUES ('Tamara' , 18 ) ;
	INSERT INTO Ages (name , age) VALUES ('Kelsay' , 31 ) ;
	INSERT INTO Ages (name , age) VALUES ('Flynn' , 33 ) ;
	
	SELECT hex(name || age) AS X FROM Ages ORDER BY X 
	
